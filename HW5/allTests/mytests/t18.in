void main() {
    // Test complex expressions
    int a = 5;
    int b = 3;
    int c = 2;
    
    // Test operator precedence
    printi(a + b * c);      // Should be 5 + (3 * 2) = 11
    printi((a + b) * c);    // Should be (5 + 3) * 2 = 16
    printi(a * b + c);      // Should be (5 * 3) + 2 = 17
    printi(a - b + c);      // Should be (5 - 3) + 2 = 4
    printi(a + b - c);      // Should be (5 + 3) - 2 = 6
}
