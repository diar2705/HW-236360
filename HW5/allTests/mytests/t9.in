void main() {
    // Test loops - for loop simulation and while
    int i = 0;
    while (i < 5) {
        printi(i);
        i = i + 1;
    }
    
    print("---");
    
    // Test nested loops
    i = 0;
    while (i < 3) {
        int j = 0;
        while (j < 2) {
            printi(i * 10 + j);
            j = j + 1;
        }
        i = i + 1;
    }
}
